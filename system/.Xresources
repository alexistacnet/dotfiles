#include </home/fuego/.termcolors/kodewolf>

Xft.dpi:                        96
Xcursor.size: 			16 
URxvt*termName:                 xterm-256color
URxvt*font:                     xft:Menlo:pixelsize=12,xft:Fira\ Mono\ for\ Powerline:pixelsize=12
URxvt.depth:                    32
URxvt*lineSpace:                1
URxvt.letterSpace:              -1
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt*geometry:                 65x17
URxvt.scrollBar:                false
URxvt*loginShell:               true
URxvt.internalBorder:           35
URxvt.clipboard.autocopy:       true

!URxvt.transparent:              true
!URxvt.shading:                  5

! Colors
*background:   black
*foreground:   white
*cursorColor:  identifier

*color0:      black
*color8:      identifier
*color1:      statement
*color9:      statement
*color2:      identifier
*color10:     value
*color3:      type 
*color11:     type
*color4:      identifier
*color12:     identifier 
*color5:      statement
*color13:     statement 
*color6:      identifier
*color14:     identifier 
*color7:      trivial 
*color15:     white

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.font: Menlo 12
rofi.color-enabled: true
rofi.padding: 400
rofi.separator-style: none
rofi.color-window: black, black, trivial_accent
rofi.color-normal: black, value, black, value, black
rofi.color-active: black, value, black, grey1, value
rofi.color-urgent: black, statement, black, grey1, statement
